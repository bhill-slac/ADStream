file NDFile.template
{
	{
    PORT            = "HDF5\$(N)",
    P               = "$(CAM):",
	R				= "$(PORT):",
    ADDR            = "\$(ADDR=0)",
    TIMEOUT         = "\$(TIMEOUT=1)",
	}
}
file NDFileAutoFileNumber.template
{
	{
    PORT            = "HDF5\$(N)",
    P               = "$(CAM):",
	R				= "$(PORT):",
	}
}
file NDFileHDF5.template
{
	{
    PORT            = "HDF5\$(N)",
    P               = "$(CAM):",
	R				= "$(PORT):",
    ADDR            = "\$(ADDR=0)",
    TIMEOUT         = "\$(TIMEOUT=1)",
    NDARRAY_PORT    = "\$(PLUGIN_SRC)",
    NDARRAY_ADDR    = "\$(NDARRAY_ADDR=0)",
	}
}


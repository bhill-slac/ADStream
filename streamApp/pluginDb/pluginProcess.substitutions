#
# pluginProcess.substitutions
#

file NDPluginBase.template
{
	{
	P=$(CAM):,
	R=Proc$(N):,
	PORT=Proc$(N),
	ADDR=0,
	TIMEOUT=1,
	NDARRAY_PORT=$(PLUGIN_SRC),
	NDARRAY_ADDR=0,
	}
}
file NDDefPluginBase.template
{
	{
	P				= "$(CAM):",
	R				= "Proc$(N):",
	NDARRAY_PORT    = $(PLUGIN_SRC),
	NDARRAY_ADDR    = 0,
	}
}

file NDProcess.template
{
	{
	P=$(CAM):,
	R=Proc$(N):,
	PORT=Proc$(N),
	ADDR=0,
	TIMEOUT=1
	}
}

file NDDefProcess.template
{
	{
	P=$(CAM):,
	R=Proc$(N):,
	}
}


